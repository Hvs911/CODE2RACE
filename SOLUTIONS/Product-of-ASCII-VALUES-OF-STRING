import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		String s1 = new String();
	    Scanner sc = new Scanner(System.in);
	    s1 = sc.nextLine();
	    int j = 1;
	    for(int i = 0; i < s1.length() ; i++) {
	        j = j * (int)s1.charAt(i);
	    }
	    System.out.println(j);
	}
}
